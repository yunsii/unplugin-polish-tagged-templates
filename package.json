{
  "name": "unplugin-polish-tagged-templates",
  "type": "module",
  "version": "0.2.0-beta.3",
  "packageManager": "pnpm@8.6.12",
  "description": "Remove unnecessary tagged templates at compile time.",
  "license": "MIT",
  "homepage": "https://github.com/yunsii/unplugin-polish-tagged-templates#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yunsii/unplugin-polish-tagged-templates.git"
  },
  "bugs": {
    "url": "https://github.com/yunsii/unplugin-polish-tagged-templates/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "tagged-template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint-staged:lint": "eslint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --check --write --no-plugin-search \"**/*.(js|jsx|tsx|ts|css|less|scss|sass|md|yaml)\""
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "consola": "^3.2.3",
    "gogocode": "^1.0.55",
    "magic-string": "^0.30.10",
    "pathe": "^1.1.1",
    "unplugin": "^1.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/eslint-config-react": "^0.35.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@jannajs/lint": "^2.3.8",
    "@nuxt/kit": "^3.6.5",
    "@nuxt/schema": "^3.6.5",
    "@types/node": "^20.5.2",
    "bumpp": "9.4.1",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "lint-staged": "^13.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^2.0.4",
    "webpack": "^5.88.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:lint",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,yaml}": [
      "prettier --write --no-plugin-search"
    ]
  }
}
