{
  "name": "unplugin-polish-tagged-templates",
  "type": "module",
  "version": "0.2.0-beta.5",
  "packageManager": "pnpm@9.10.0",
  "description": "Remove unnecessary tagged templates at compile time.",
  "license": "MIT",
  "homepage": "https://github.com/yunsii/unplugin-polish-tagged-templates#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yunsii/unplugin-polish-tagged-templates.git"
  },
  "bugs": {
    "url": "https://github.com/yunsii/unplugin-polish-tagged-templates/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "tagged-template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint --flag unstable_ts_config .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepare": "husky init",
    "lint:fix": "eslint --flag unstable_ts_config --fix ."
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "consola": "^3.2.3",
    "gogocode": "^1.0.55",
    "magic-string": "^0.30.10",
    "pathe": "^1.1.1",
    "unplugin": "^1.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint-react/eslint-plugin": "^1.14.1",
    "@jannajs/lint": "3.0.0-next.20",
    "@nuxt/kit": "^3.6.5",
    "@nuxt/schema": "^3.6.5",
    "@types/node": "^20.5.2",
    "bumpp": "9.4.1",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "eslint": "^9.10.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^2.0.4",
    "webpack": "^5.88.2"
  },
  "lint-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
